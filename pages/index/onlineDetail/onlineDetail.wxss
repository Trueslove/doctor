}
.detail-chat-txt.detail-chat-image{
  padding: 0;
  border-radius: 4rpx;
  overflow: hidden;
}
.detail-chat-txt.detail-chat-image:after,
.detail-chat-txt.detail-chat-video:after{
  display: none;
}
.detail-chat-txt.detail-chat-video{
  padding: 0; 
}
.detail-chat-txt.detail-chat-video video{
  width: 360rpx;
  height: 360rpx;
}

.detail-chat-l-txt::before {
  content: "";
  position: absolute;
  left: -32rpx;
  top: 12rpx;
  width: 0;
  height: 0;
  border-top: 20rpx solid transparent;
  border-bottom: 20rpx solid transparent;
  border-left: 20rpx solid transparent;
  border-right: 20rpx solid #f2f2f2;
}

.detail-chat-r-txt::after {
  content: "";
  position: absolute;
  right: -32rpx;
  top: 12rpx;
  width: 0;
  height: 0;
  border-top: 20rpx solid transparent;
  border-bottom: 20rpx solid transparent;
  border-left: 20rpx solid #f2f2f2;
  border-right: 20rpx solid transparent;
  z-index: -1;
}

